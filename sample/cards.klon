Rank := List clone do(
	for(rank,2,10,add(rank asString))
	add("Jack") add("Queen") add("King") add("Ace")
)

Suit := list("Clubs", "Diamonds", "Hearts", "Spades")

Card := Object clone do(
	init := method(suit, rank,
		self suit := suit;
		self rank := rank
	)
	asString = method(rank + " of " + suit)
)

Deck := List clone do(
	init := method(
			Suit forEach(suit,
				Rank forEach(rank,
					add(Card clone(suit, rank))
				)
			)
		)
	draw := method(pop)
)
